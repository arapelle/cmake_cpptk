
@PACKAGE_INIT@

include(${CMAKE_CURRENT_LIST_DIR}/@PACKAGE_NAME@-targets.cmake)

foreach(component ${@PROJECT_NAME@_FIND_COMPONENTS})
    if(@PROJECT_NAME@_FIND_REQUIRED_${component})
        message(SEND_ERROR "Missing required component @PROJECT_NAME@::${component}!")
    else()
        message(WARNING "Missing optional component @PROJECT_NAME@::${component}.")
    endif()
endforeach()

check_required_components(@PROJECT_NAME@)

if(NOT DEFINED @PROJECT_NAME@_FOUND)
    set(@PROJECT_NAME@_FOUND TRUE)
    include(CMakePrintHelpers)
    message(STATUS "Found package @PROJECT_NAME@/@PROJECT_VERSION@")
endif()
